/**
 * 
 */
package es.indra.aerolineas;

/**
 * @author josejarizav
 *
 */
public class Aerolinea {
	
	int id;
	String nombre;
	Vuelo[] vuelos;
	
	public Aerolinea() {		
	}

	public Aerolinea(int id, String nombre, Vuelo[] vuelos) {
		super();
		this.id = id;
		this.nombre = nombre;
		this.vuelos = vuelos;
	}
	
	

}
